{
  "rewrites": [
    {
      "source": "/s/:shortCode",
      "destination": "https://task-manager-backend-5v7c.onrender.com/s/:shortCode"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}